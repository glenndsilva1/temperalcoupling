package test;

public class MailInfo {
	private final String to;
	private final String from;
	private final String subject;
	private final String body;
	
	public MailInfo(String to, String from, String subject, String body) {
		this.to = to;
		this.from = from;
		this.subject = subject;
		this.body = body;
	}
	
	
	public String to() {
		return this.to;
	}
	
	public String from() {
		return this.from;
	}
	
	public String subject() {
		return this.subject;
	}
	
	public String body() {
		return this.body;
	}
	
	public String toString() {
		return this.to + " "  + this.from + " "+ this.subject + " "+ this.body;
	}
}
